{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "object-curly-spacing": ["error", "never"],
        "no-console": "off",
        "no-shadow": "off",
        "linebreak-style": "off",
        "indent": ["error", 4],
        "no-underscore-dangle": "off",
        "consistent-return":"warn",
        "camelcase":"warn"
    }
}
